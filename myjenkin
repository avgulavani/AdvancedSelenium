node{

    stage('Build Jar') {
            agent {
                docker {
                    image 'maven:3-alpine'
                    args '-v $HOME/.m2:/root/.m2'
                }
            }
            steps {
                 	sh 'mvn clean package -DskipTests'
                }
    }

    stage('SCM checkout'){
        git 'https://github.com/avgulavani/AdvancedSelenium.git'
    }
    stage('Compile package'){   
        sh 'mvn package'
    }
}
